
@{
    ViewBag.Title = "Listar";
}

<table class="table">
    <thead>
        <tr>
            <th>Nome</th>
            <th>Idade</th>
            <th>Crédito</th>
        </tr>
            
    </thead>
    <tbody>
        @foreach(var item in ViewBag.lista)
        {
            <tr>
                <td>@item.Nome</td>
                <td>@item.Idade</td>
                <td>@item.Credito</td>
            </tr>

        }

    </tbody>
</table>

